<?xml version='1.0' encoding='utf-8'?>
<widget id="io.jmc.datakudiy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Dataku DIY</name>
    <description>
        Aplikasi Dataku Bappeda Daerah Istimewa Yogyakarta
    </description>
    <author email="support@jmc.co.id" href="http://jmc.co.id">
        JMC Mobile Apps Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="StatusBarBackgroundColor" value="#1d719a" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon platform="android" qualifier="ldpi" src="res/icon.png" />
        <icon platform="android" qualifier="mdpi" src="res/icon.png" />
        <icon platform="android" qualifier="hdpi" src="res/icon.png" />
        <icon platform="android" qualifier="xhdpi" src="res/icon.png" />
        <icon platform="android" qualifier="xxhdpi" src="res/icon.png" />
        <icon platform="android" qualifier="xxxhdpi" src="res/icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="^2.1.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
